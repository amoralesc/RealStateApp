application.name=Real State App
application.description=REST API for a Real State App
spring.mvc.pathmatch.matching-strategy=ANT_PATH_MATCHER

datasource.url=[replace with database url]
datasource.username=[replace with database username]
datasource.password=[replace with database password]

rabbit.host=[Server host]
rabbit.port=[Server port]
rabbit.user=[RabbitMQ username]
rabbit.password=[RabbitMQ password]
rabbit.exchange=realstate_domain_events
rabbit.queues=realstate.appointments.appointment.create.appointment,realstate.appointments.request.create.request,realstate.appointments.appointment.update.appointment,realstate.appointments.request.update.request
